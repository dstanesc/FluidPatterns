{
    "compilerOptions": {
        "target": "es2017",
        "lib": ["ES2017", "ES2018.Promise", "DOM", "DOM.Iterable"],
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": false,
        "allowJs": true,
        "noImplicitOverride": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "incremental": true,
        "skipLibCheck": true
    }
}
